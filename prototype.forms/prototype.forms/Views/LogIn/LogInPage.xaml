<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="prototype.forms.Views.LogIn.LogInPage"
             Title="Log In">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout VerticalOptions="EndAndExpand">
                <Button Margin="40" Text="Log In" Command="{Binding LogInCommand}"></Button>
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>